        "num=int(input())\n",
        "sum=0\n",
        "count=0;\n",
        "while (sum!=1):\n",
        "  while(num>=10):\n",
        "    sum=0\n",
        "    sum=sum+(num%10)*(num%10)\n",
        "    num=int(num/10)\n",
        "  if((num%10)!=0):\n",
        "    sum=sum+(num%10)*(num%10)\n",
        "  num=sum\n",
        "  count+=1\n",
        "  if(count==1000):\n",
        "    print(\"False\")\n",
        "    break;\n",
        "if(sum==1):\n",
        "  print(\"True\")"
      ]
